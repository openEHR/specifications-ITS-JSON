                "link_perfomed_action": [],
                "etc": []
            }
        },
        "I_TASK_PLAN_MATERIALISED": {
            "title": "I_TASK_PLAN_MATERIALISED",
            "description": "Task planning API for materialised Task Plan level.",
            "$comments": "Defined in Service IM",
            "$functions": {
                "materialise": [],
                "activate": [],
                "terminate": []
            }
