                        }
                    }
                },
                "plan": {
                    "description": "Obtain a Task Plan definition API interface.",
                    "parameters": {
                        "a_task_plan_id": {
                            "$ref": "https://www.openEHR.org/releases/ITS-JSON/latest/components/BASE/latest/Base_types/all.json#/definitions/UID_BASED_ID"
                        }
                    },
                    "required_parameters": [
                        "a_task_plan_id"
                    ],
                    "return": {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/I_TASK_PLAN_DEFINITION"
                        }
                    }
                }
            }
        }
